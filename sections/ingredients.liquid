{%- liquid
  assign bg_color = section.settings.background_color | default: settings.color_background
  assign text_color = section.settings.text_color | default: settings.color_text
  assign accent = section.settings.accent_color | default: settings.color_primary

  assign subheading_color = section.settings.subheading_color | default: text_color
  assign heading_color = section.settings.heading_color | default: accent
  assign name_color = section.settings.name_color | default: text_color
  assign desc_color = section.settings.desc_color | default: text_color
  assign overlay_tag_bg = section.settings.overlay_tag_bg | default: accent
  assign overlay_tag_text = section.settings.overlay_tag_text | default: '#ffffff'
  assign bottom_tag_border = section.settings.bottom_tag_border | default: text_color
  assign bottom_tag_text = section.settings.bottom_tag_text | default: text_color
-%}

<section
  class="ingredients full-width"
  style="
    --ing-bg: {{ bg_color }};
    --ing-text: {{ text_color }};
    --ing-accent: {{ accent }};

    --ing-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
    --ing-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
    --ing-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
    --ing-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;

    --ing-header-spacing-mobile: {{ section.settings.header_spacing_mobile }}px;
    --ing-header-spacing-desktop: {{ section.settings.header_spacing_desktop }}px;
    --ing-gap: {{ section.settings.gap }}px;

    --ing-subheading-color: {{ subheading_color }};
    --ing-subheading-size-mobile: {{ section.settings.subheading_size_mobile }}px;
    --ing-subheading-size-desktop: {{ section.settings.subheading_size_desktop }}px;
    --ing-subheading-weight: {{ section.settings.subheading_weight }};

    --ing-heading-color: {{ heading_color }};
    --ing-heading-size-mobile: {{ section.settings.heading_size_mobile }}px;
    --ing-heading-size-desktop: {{ section.settings.heading_size_desktop }}px;
    --ing-heading-weight: {{ section.settings.heading_weight }};

    --ing-name-color: {{ name_color }};
    --ing-name-size-mobile: {{ section.settings.name_size_mobile }}px;
    --ing-name-size-desktop: {{ section.settings.name_size_desktop }}px;
    --ing-name-weight: {{ section.settings.name_weight }};
    --ing-name-spacing-mobile: {{ section.settings.name_spacing_mobile }}px;
    --ing-name-spacing-desktop: {{ section.settings.name_spacing_desktop }}px;

    --ing-desc-color: {{ desc_color }};
    --ing-desc-size-mobile: {{ section.settings.desc_size_mobile }}px;
    --ing-desc-size-desktop: {{ section.settings.desc_size_desktop }}px;
    --ing-desc-weight: {{ section.settings.desc_weight }};
    --ing-desc-spacing-mobile: {{ section.settings.desc_spacing_mobile }}px;
    --ing-desc-spacing-desktop: {{ section.settings.desc_spacing_desktop }}px;

    --ing-overlay-tag-bg: {{ overlay_tag_bg }};
    --ing-overlay-tag-text: {{ overlay_tag_text }};
    --ing-overlay-tag-size-mobile: {{ section.settings.overlay_tag_size_mobile }}px;
    --ing-overlay-tag-size-desktop: {{ section.settings.overlay_tag_size_desktop }}px;

    --ing-bottom-tag-border: {{ bottom_tag_border }};
    --ing-bottom-tag-text: {{ bottom_tag_text }};
    --ing-bottom-tag-size-mobile: {{ section.settings.bottom_tag_size_mobile }}px;
    --ing-bottom-tag-size-desktop: {{ section.settings.bottom_tag_size_desktop }}px;

    --ing-arrow-color: {{ section.settings.arrow_color | default: text_color }};
    --ing-arrow-size: {{ section.settings.arrow_size }}px;
  "
>
  {% if section.settings.heading != blank or section.settings.subheading != blank %}
    <header class="ingredients__header">
      <div class="ingredients__header-text">
        {% if section.settings.subheading != blank %}
          <p class="ingredients__subheading">{{ section.settings.subheading }}</p>
        {% endif %}
        {% if section.settings.heading != blank %}
          <h2 class="ingredients__heading">{{ section.settings.heading }}</h2>
        {% endif %}
      </div>
      {% if section.blocks.size > 1 %}
        <div class="ingredients__nav">
          <button type="button" class="ingredients__arrow ingredients__arrow--prev" aria-label="Previous" data-direction="prev">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
              <path d="M15 18l-6-6 6-6"/>
            </svg>
          </button>
          <button type="button" class="ingredients__arrow ingredients__arrow--next" aria-label="Next" data-direction="next">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
              <path d="M9 18l6-6-6-6"/>
            </svg>
          </button>
        </div>
      {% endif %}
    </header>
  {% endif %}

  {% if section.blocks.size > 0 %}
    <div class="ingredients__track">
      <div class="ingredients__grid" data-ingredients-grid>
        {% for block in section.blocks %}
          <article class="ingredients__card" {{ block.shopify_attributes }}>
            <div class="ingredients__image-wrap">
              {% if block.settings.image != blank %}
                <img
                  src="{{ block.settings.image | image_url: width: 600 }}"
                  alt="{{ block.settings.name | escape }}"
                  loading="lazy"
                  class="ingredients__img"
                >
              {% else %}
                <div class="ingredients__placeholder"></div>
              {% endif %}

              {% if block.settings.overlay_tag != blank %}
                <span class="ingredients__overlay-tag">{{ block.settings.overlay_tag }}</span>
              {% endif %}
            </div>

            <div class="ingredients__content">
              {% if block.settings.name != blank %}
                <h3 class="ingredients__name">{{ block.settings.name }}</h3>
              {% endif %}

              {% if block.settings.description != blank %}
                <p class="ingredients__desc">{{ block.settings.description }}</p>
              {% endif %}

              {% if block.settings.tag_1 != blank %}
                <div class="ingredients__tags">
                  <span class="ingredients__tag">{{ block.settings.tag_1 }}</span>
                  {% if block.settings.tag_2 != blank %}
                    <span class="ingredients__tag">{{ block.settings.tag_2 }}</span>
                  {% endif %}
                  {% if block.settings.tag_3 != blank %}
                    <span class="ingredients__tag">{{ block.settings.tag_3 }}</span>
                  {% endif %}
                </div>
              {% endif %}
            </div>
          </article>
        {% endfor %}
      </div>
    </div>
  {% endif %}
</section>

{% stylesheet %}
  .ingredients {
    background: var(--ing-bg);
    color: var(--ing-text);
    padding: var(--ing-padding-top-mobile) var(--page-margin) var(--ing-padding-bottom-mobile);
  }

  .ingredients__header {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
    gap: var(--space-lg);
    margin-bottom: var(--ing-header-spacing-mobile);
  }

  .ingredients__header-text {
    text-align: left;
  }

  .ingredients__subheading {
    font-family: var(--font-body-family);
    font-size: var(--ing-subheading-size-mobile);
    font-weight: var(--ing-subheading-weight);
    color: var(--ing-subheading-color);
    margin: 0 0 4px;
    opacity: 0.7;
  }

  .ingredients__heading {
    font-family: var(--font-heading-family);
    font-size: var(--ing-heading-size-mobile);
    font-weight: var(--ing-heading-weight);
    color: var(--ing-heading-color);
    margin: 0;
    letter-spacing: -0.01em;
  }

  .ingredients__nav {
    display: flex;
    gap: 8px;
    flex-shrink: 0;
  }

  .ingredients__arrow {
    display: flex;
    align-items: center;
    justify-content: center;
    width: var(--ing-arrow-size);
    height: var(--ing-arrow-size);
    background: transparent;
    border: 1px solid var(--ing-arrow-color);
    border-radius: 50%;
    color: var(--ing-arrow-color);
    cursor: pointer;
    transition: all 0.2s ease;
    padding: 0;
  }

  .ingredients__arrow:hover {
    background: var(--ing-arrow-color);
    color: var(--ing-bg);
  }

  .ingredients__arrow svg {
    width: 50%;
    height: 50%;
  }

  .ingredients__track {
    overflow: hidden;
  }

  .ingredients__grid {
    display: flex;
    gap: var(--ing-gap);
    overflow-x: auto;
    scroll-snap-type: x mandatory;
    -webkit-overflow-scrolling: touch;
    scrollbar-width: none;
    scroll-behavior: smooth;
  }

  .ingredients__grid::-webkit-scrollbar {
    display: none;
  }

  .ingredients__card {
    flex: 0 0 220px;
    scroll-snap-align: start;
  }

  @media (min-width: 768px) {
    .ingredients__card {
      flex: 0 0 260px;
    }
  }

  @media (min-width: 1024px) {
    .ingredients__card {
      flex: 0 0 280px;
    }
  }

  .ingredients__image-wrap {
    position: relative;
    overflow: hidden;
    margin-bottom: var(--ing-name-spacing-mobile);
  }

  .ingredients__img {
    display: block;
    width: 100%;
    aspect-ratio: 3 / 4;
    object-fit: cover;
  }

  .ingredients__placeholder {
    width: 100%;
    aspect-ratio: 3 / 4;
    background: linear-gradient(135deg, var(--ing-accent) 0%, transparent 80%);
    opacity: 0.15;
  }

  .ingredients__overlay-tag {
    position: absolute;
    top: var(--space-sm);
    left: var(--space-sm);
    background: var(--ing-overlay-tag-bg);
    color: var(--ing-overlay-tag-text);
    font-size: var(--ing-overlay-tag-size-mobile);
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.06em;
    padding: 6px 10px;
    line-height: 1;
  }

  .ingredients__content {
    text-align: left;
  }

  .ingredients__name {
    font-family: var(--font-heading-family);
    font-size: var(--ing-name-size-mobile);
    font-weight: var(--ing-name-weight);
    color: var(--ing-name-color);
    margin: 0 0 var(--ing-desc-spacing-mobile);
    letter-spacing: -0.01em;
  }

  .ingredients__desc {
    font-family: var(--font-body-family);
    font-size: var(--ing-desc-size-mobile);
    font-weight: var(--ing-desc-weight);
    color: var(--ing-desc-color);
    margin: 0 0 var(--ing-desc-spacing-mobile);
    opacity: 0.8;
    line-height: 1.5;
  }

  .ingredients__tags {
    display: flex;
    flex-wrap: wrap;
    gap: 6px;
  }

  .ingredients__tag {
    display: inline-block;
    background: transparent;
    border: 1px solid var(--ing-bottom-tag-border);
    color: var(--ing-bottom-tag-text);
    font-size: var(--ing-bottom-tag-size-mobile);
    font-weight: 500;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    padding: 4px 8px;
    line-height: 1;
  }

  @media (min-width: 768px) {
    .ingredients {
      padding: var(--ing-padding-top-desktop) var(--page-margin) var(--ing-padding-bottom-desktop);
    }

    .ingredients__header {
      margin-bottom: var(--ing-header-spacing-desktop);
    }

    .ingredients__subheading {
      font-size: var(--ing-subheading-size-desktop);
    }

    .ingredients__heading {
      font-size: var(--ing-heading-size-desktop);
    }

    .ingredients__image-wrap {
      margin-bottom: var(--ing-name-spacing-desktop);
    }

    .ingredients__name {
      font-size: var(--ing-name-size-desktop);
      margin-bottom: var(--ing-desc-spacing-desktop);
    }

    .ingredients__desc {
      font-size: var(--ing-desc-size-desktop);
      margin-bottom: var(--ing-desc-spacing-desktop);
    }

    .ingredients__overlay-tag {
      font-size: var(--ing-overlay-tag-size-desktop);
    }

    .ingredients__tag {
      font-size: var(--ing-bottom-tag-size-desktop);
    }
  }
{% endstylesheet %}

{% javascript %}
  (function() {
    const section = document.currentScript.closest('.shopify-section');
    if (!section) return;

    const grid = section.querySelector('[data-ingredients-grid]');
    const prevBtn = section.querySelector('.ingredients__arrow--prev');
    const nextBtn = section.querySelector('.ingredients__arrow--next');

    if (!grid || !prevBtn || !nextBtn) return;

    const getScrollAmount = () => {
      const card = grid.querySelector('.ingredients__card');
      if (!card) return 280;
      const style = getComputedStyle(grid);
      const gap = parseInt(style.gap) || 16;
      return card.offsetWidth + gap;
    };

    prevBtn.addEventListener('click', () => {
      grid.scrollBy({ left: -getScrollAmount(), behavior: 'smooth' });
    });

    nextBtn.addEventListener('click', () => {
      grid.scrollBy({ left: getScrollAmount(), behavior: 'smooth' });
    });
  })();
{% endjavascript %}

